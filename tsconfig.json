{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@game.tsx": [
        "app/game.tsx"
      ],
      "@/*": [
        "*"
      ],
      "@app/*": [
        "app/*"
      ],
      "@src/*": [
        "src/*"
      ],
      "@assets/*": [
        "assets/*"
      ]
    },
    "types": [
      "react",
      "react-native",
      "expo-router"
    ]
  },
  "include": [
    "app",
    "src"
  ]
}
